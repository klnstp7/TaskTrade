<script type="text/javascript">
    $(function () {
        //跳转到立项页面
        $("#attendto").click(function () {
            var str = $(this).attr("tag");
            var fram = parent.document.getElementById("url");
            fram.src = str;
        });
        //验证
        var index, confirm, dialog;
        $("#addinfo").validate({
            rules: {
                //客户手机
                mobilephone: {
                    isMobile: true,
                },
                //固定电话
                fixedtelephone: {
                    isTel: true,
                },
                //客户姓名
                customername: {
                    stringCheck: true,
                    maxlength: 50,
                },
                //客户qq
                customerqq: {
                    isDigits: true,
                    maxlength: 12,
                    minlength:5
                },
                //小区名称
                communityname:{
                    required:true

                },
                //建筑面积
                areas:{
                    required: true,
                    posintdec:true               
                }
            },
            messages: {
                mobilephone: {
                    isMobile: "输入正确格式的手机号"
                },
                fixedtelephone: {
                    isTel: "输入输入正确格式电话号码"
                },
                customername: {
                    stringCheck: "请输入正确格式的客户姓名",
                    maxlength: jQuery.validator.format("字符不能超过50个")
                },
                customerqq: {
                    isDigits: "请输入正确格式qq号码",
                    maxlength:jQuery.validator.format("字符不能超过12个"),
                    minlength:jQuery.validator.format("字符不能少于5个")
                },
                communityname: {
                    required: "请输入小区名称"
                },
                areas: {
                    required: "请输入面积",
                    posintdec: "必须数字(最多输入两位小数)"

                }
            },
            success: function (label) {
                layer.close(dialog);
                $("#" + label[0].htmlFor).removeClass("error-border");
            },
            errorPlacement: function (error, element) {
                $("#" + element[0].id).addClass("error-border");
                dialog = layer.tips(error[0].innerHTML, "#" + element[0].id, {
                    tips: [2, '#F99228']
                });
            }, submitHandler: function () {
                alert("提交事件!");
            }
        });
    })
</script>